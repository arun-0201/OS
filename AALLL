ss.jsp
<%
int x=Integer.parseInt(request.getParameter("txtNum1"));
int y= Integer.parseInt(request.getParameter("txtNum2"));
String z =request.getParameter("operation");%>
<%!
int calc(int m ,int n,String s)
{
if(s.equals("+"))
{
return (m+n);
}
else if(s.equals("-"))
{
return (m-n);
}
else if(s.equals("*"))
{
return (m*n);
}
else if(s.equals("/"))
{
return (m/n);
}
else{
return -1;
}
}
%>
<%=calc(x,y,z)%>
