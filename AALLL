<!DOCTYPE html>
<html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<body>
<div id="myTable"></div>
<script>
$(document).ready(function() {
$.ajax({
url: 'file.json',
type:"GET",
dataType: 'json',
success: function(data) {
for (var i=0; i<data.length; i++) {
var row = $('<table border="1"><tr><td>' + data[i].zipcode+ '</td><td>' + data[i].city + '</td><td>'
+ data[i].county + '</td></tr></table>');
$('#myTable').append(row);
}
},
});
});
</script>
</body>
</html>
