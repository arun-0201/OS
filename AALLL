<!DOCTYPE html>
<html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<body>
<form ><br><br>
name :<input type="text" name="txtName" id="txtName"/><br><br>
<input type="submit" value="submit" id="save_data"/>
<div id="divStatus"></div>
</form>
<script>
$(document).ready(function() {
$("#save_data").click(function() {
$.ajax({
url: "save.jsp",
type: "post",
data: {
name:$('#txtName').val()
}
}).done(function(status, text){
//$("#divStatus").html(status);
alert(status);alert(text);
});
});
});
document.write(name);
</script>
</body>
</html>
Save.jsp
<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
pageEncoding="ISO-8859-1"%>
<%@page import="java.sql.*,java.util.*"%>
<%
String name = request.getParameter("name");
out.print(name);
%>
<!DOCTYPE html>
<html>
<head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>
$(document).ready(function(){
$("button").click(function(){
$.ajax({url:"jq.txt",type:"GET",success:function(result){
//$("#div1").text(result);
var x=result.split("\n");
//document.write(x.length);
for(i=0;i<x.length;i++)
{
$("#div1").append(x[i]);
}
}});
});
});
</script>
</head>
<body>
<div id="div1">
</div>
<button>Get External Content</button>
</body>
</html>
