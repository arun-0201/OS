<%!
    String getCustomerData(){                
           File file = new File("E:\\ss.txt");
 
        try (BufferedReader br = new BufferedReader(new FileReader(file)))
        {
            String line,line1="";
            while ((line = br.readLine()) != null) {
               line1+=line; 
            }
			return line1;
        } catch (IOException e) {
            e.printStackTrace(); return "xx";
        }
    }        
%>
