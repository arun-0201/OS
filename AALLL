</form>
<script>
$(document).ready(function() {
$("#save_data").click(function() {

$.ajax({
url: "save.jsp",
type: "post",
data: {
name:$('#txtName').val(),
address:$('#txtAddress').val(),
city:$('#txtCity').val(),

state:$('#txtState').val(),
zipCode:$('#txtZipCode').val(),
phone:$('#txtPhone').val(),
email:$('#txtEmail').val(),
}
});
});
});
